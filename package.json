    "webdriver-manager": "^1.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.33",
    "@types/minimist": "^1.1.29",
    "@types/node": "^6.0.45",
    "body-parser": "1.14.2",
    "clang-format": "^1.0.34",
    "express": "~4.13.3",
    "gulp": "^3.9.1",
    "gulp-clang-format": "^1.0.23",
    "jasmine": "2.3.2",
    "jshint": "2.9.1",
    "webdriver-manager": "^1.0.1"
  },
  "repository": {
    "type": "git",
